        </mxCell>
        <mxCell id="xANYkGlC_JNNwY0ZGK-Z-19" value="y = FirstClass()&amp;nbsp; &amp;nbsp; &amp;nbsp; # Each is a new namespace &lt;br&gt;&lt;table class=&quot;NormalTable&quot;&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td width=&quot;212&quot;&gt;&lt;br&gt;y.setdata(3.14159)&lt;br&gt;&lt;br&gt;y.setdata(2.1415)&amp;nbsp; &amp;nbsp; &amp;nbsp;# y.setdata(self=y, value=2.1415)&lt;br&gt;&lt;br&gt;y.display()&lt;br&gt;&lt;br&gt;y.addValue(2)&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;# y.addValue(self=y, value=2)&lt;br&gt;&lt;br&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt; &lt;div&gt;y.setInstanceName(&quot;instance y&quot;)&lt;/div&gt;" style="text;html=1;fontFamily=Courier New;" vertex="1" parent="1">
