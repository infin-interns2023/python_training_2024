        <mxCell id="xANYkGlC_JNNwY0ZGK-Z-18" value="x = FirstClass()         &#xa;&#xa;x.setdata(&quot;King Arthur&quot;) # Call methods: self is x &#xa;&#xa;&#xa;x.setdata(&quot;New value&quot;)   # x.setdata(self=x, value=2.1415)&#xa;&#xa;x.display()&#xa;&#xa;x.addValue(&quot; has a sword&quot;)&#xa;&#xa;x.setInstanceName(&quot;instance x&quot;)&#xa;" style="text;fontFamily=Courier New;" vertex="1" parent="1">
